{
    "types": [
        "DATE",
        "POSTAL_CODE",
        "REFERENCE",
        "PACKAGE",
        "SMALL",
        "NUMBER"
    ],
    "patterns": [
        {
            "pattern": "^([0-9\\-]{10,})$",
            "type": "REFERENCE"
        },
        {
            "pattern": "^([0-9,]+(\\.[0-9]+)?\\s*[mglptcdMGLPTCD정캅포매]+)",
            "type": "PACKAGE"
        },
        {
            "pattern": "^([0-9]{5,6}|[0-9]{3}-[0-9]{2,3})$",
            "type": "POSTAL_CODE"
        },
        {
            "pattern": "^([0-9]{4,8}|[0-9]{1,4}[/-][0-9]{1,2}[/-][0-9]{1,2}|[0-9]{1,4}[/-년][0-9]{1,2})[월]?$",
            "type": "DATE"
        },
        {
            "pattern": "^(-?[0-9,]+(\\.[0-9]+)?)$",
            "type": "NUMBER"
        },
        {
            "pattern": "^(-?[0-9,]{1,3}(\\.[0-9]+)?)$",
            "type": "SMALL"
        }
    ],
    "maxVectorSize": 24
}
