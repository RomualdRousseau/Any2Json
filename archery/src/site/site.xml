<project xmlns="http://maven.apache.org/DECORATION/1.7.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.7.0 http://maven.apache.org/xsd/decoration-1.7.0.xsd"
         name="Archery">

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.11.1</version>
    </skin>

    <bannerLeft>
        <name>Archery</name>
        <src>images/archery-logo.png</src>
        <href>https://romualdrousseau.github.io/Archery/</href>
    </bannerLeft>

    <body>
        <menu ref="reports"/>
        <footer><![CDATA[@ 2024 Romuald Rousseau - All rights reserved]]></footer>
    </body>

    <custom>
        <fluidoSkin>
            <gitHub>
                <projectId>romualdrousseau/Archery</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>gray</ribbonColor>
            </gitHub>
            <copyrightClass>pull-right</copyrightClass>
        </fluidoSkin>
    </custom>

</project>
